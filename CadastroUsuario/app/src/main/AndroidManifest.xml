<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cadastrousuario">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_floppy_disk"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_floppy_disk"
        android:supportsRtl="true"
        android:theme="@style/Theme.CadastroUsuario">

        <activity android:name=".CadastroActivity"
            android:label="@string/titulo_cadastro">
        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>